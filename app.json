{
  "expo": {
    "name": "BEA",
    "slug": "bea",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "studio-lash",
    "owner": "midnight2606",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.anonymous.LashApp",
      "versionCode": 1,
      "googleServicesFile": "./android/app/google-services.json",
      "permissions": [
        "INTERNET",
        "CAMERA",
        "VIBRATE",
        "NOTIFICATIONS"
      ],
      "navigationBar": {
        "visible": true,
        "backgroundColor": "#FF69B4",
        "barStyle": "transparent",
        "barStyleOnEdge": "transparent",
        "behavior": "normal"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#000"
        }
      ],
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f26b9ad7-cf2f-4a8f-a6e1-6929c976d62e"
      }
    }
  }
}
