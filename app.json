{
  "expo": {
    "name": "BEA",
    "slug": "beauty-easy-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "studio-lash",
    "owner": "midnightph",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.LashApp", 
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.anonymous.LashApp",
      "versionCode": 1,
      "permissions": ["NOTIFICATIONS"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#000000"
      },
      "navigationBar": {
        "visible": true,
        "backgroundColor": "#FF69B4",
        "barStyle": "transparent",
        "barStyleOnEdge": "transparent",
        "behavior": "normal"
      },
      "edgeToEdgeEnabled": false,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#000"
        }
      ],
      "expo-notifications"
    ],
    "extra": {
      "eas": {
        "projectId": "SEU_PROJECT_ID_AQUI"
      }
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
