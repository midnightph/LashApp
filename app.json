{
  "expo": {
    "name": "BEA",
    "slug": "Beauty Easy App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "studio-lash",
    "owner": "midnightph",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "navigationBar": {
        "visible": true,
        "backgroundColor": "#FF69B4",
        "barStyle": "transparent",
        "barStyleOnEdge": "transparent",
        "behavior": "normal"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#FFF2F5"
      },
      "edgeToEdgeEnabled": false,
      "package": "com.anonymous.LashApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#000"
        }
      ],
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
