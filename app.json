{
  "expo": {
    "name": "BEA",
    "slug": "bea",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "studio-lash",
    "owner": "midnight2606",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.company.bea",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.company.bea",
      "versionCode": 1,
      "permissions": ["NOTIFICATIONS"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#000000"
      },
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      "expo-notifications",
      ["expo-build-properties", {
        "android": {
          "gradlePluginVersion": "8.4.1",
          "kotlinVersion": "2.0.21"
        }
      }]
    ],
    "extra": {
      "eas": {
        "projectId": "f26b9ad7-cf2f-4a8f-a6e1-6929c976d62e"
      }
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
